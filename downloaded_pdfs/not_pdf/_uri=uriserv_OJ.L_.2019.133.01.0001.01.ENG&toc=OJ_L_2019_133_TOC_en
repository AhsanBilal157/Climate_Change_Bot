                    "counter": false,
                    "css": {
                        "button": "myButton"
                    },
                    "link": "https://eur-lex.europa.eu/legal-content/EN/TXT/?toc=OJ:L:2019:133:TOC|en&amp;uri=uriserv:OJ.L_.2019.133.01.0001.01.ENG"
